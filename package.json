{
  "name": "kelas-masjid-al-muwahiddin",
  "productName": "Kelas Masjid Al-Muwahiddin Medan",
  "version": "1.0.0",
  "description": "Listing kelas dan form registrasi kelas belajar di Masjid Al-Muwahiddin Medan",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev:css": "tailwindcss -i ./app/input.css -o ./app/output.css --watch",
    "build:css": "tailwindcss -i ./app/input.css -o ./app/output.css",
    "build": "npm run build:css && remix vite:build",
    "dev": "concurrently \"npm run dev:css\" \"remix vite:dev\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "npm run build:css && netlify serve",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@netlify/functions": "^2.6.0",
    "@netlify/remix-adapter": "^2.3.1",
    "@remix-run/node": "^2.15.3",
    "@remix-run/react": "^2.15.3",
    "@remix-run/serve": "^2.15.3",
    "@supabase/supabase-js": "^2.48.1",
    "animate.css": "^4.1.1",
    "axios": "^1.7.9",
    "isbot": "^4.1.0",
    "react-animate-on-scroll": "^2.1.9",
    "react-hook-form": "^7.54.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.1.6",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "Remix",
    "React",
    "React Js"
  ],
  "author": {
    "name": "BOPI CORPS",
    "email": "daulayreza@gmail.com"
  }
}
